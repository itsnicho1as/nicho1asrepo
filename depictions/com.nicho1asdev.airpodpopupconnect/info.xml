		</screenshot>
		<screenshot>
			<description>Apple-style with a done button</description>
			<image>apple-style+done.jpg</image>
		</screenshot>
		<screenshot>
			<description>Plain</description>
			<image>plain-style.jpg</image>
		</screenshot>
		<screenshot>
			<description></description>
			<image>settings.jpg</image>
		</screenshot>
	</screenshots>
	<changelog>
		<change>Rewritten to support iOS 14!</change>
	</changelog>
